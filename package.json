{
  "name": "rimmel",
  "version": "1.2.3",
  "description": "A Functional-Reactive UI library for the Rx.Observable Universe",
  "type": "module",
  "_main": "dist/cjs/index.cjs",
  "module": "dist/esm/rimmel.mjs",
  "types": "dist/esm/modules/index.d.ts",
  "paths": {
    "rimmel/ssr": [
      "dist/ssr"
    ],
    "rimmel/ssr/*": [
      "dist/ssr"
    ]
  },
  "exports": {
    ".": {
      "import": "./dist/esm/rimmel.mjs"
    },
    "./ssr": {
      "import": "./dist/ssr/ssr.mjs",
      "require": "./dist/ssr/ssr.cjs"
    }
  },
  "files": [
    "dist/",
    "favicon.svg"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && rollup --bundleConfigAsCjs --config rollup.config.ts --configPlugin typescript",
    "dev": "rimraf dist && rollup -w --bundleConfigAsCjs --config rollup.config.ts --configPlugin typescript",
    "website:local": "bundle exec jekyll serve",
    "test": "bun test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactivehtml/rimmel.git"
  },
  "keywords": [
    "rimmel",
    "rimmel.js",
    "rimmeljs",
    "rxjs",
    "rx",
    "html",
    "reactive",
    "reactive html",
    "reactive dom",
    "rml",
    "fp",
    "frp",
    "functional programming",
    "functional-reactive",
    "functional-reactive programming"
  ],
  "author": "Dario Mannu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactivehtml/rimmel/issues"
  },
  "homepage": "https://rimmel.js.org",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "esbuild": "^0.19.11",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^4.9.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.9"
  },
  "peerDependencies": {
    "rxjs": ">=5.5.0 || >=6.0.0 || >=7.0.0"
  }
}
